# Sample makefile

CC = g++
CCFLAGS = -DLINUX -I/opt/3DGuidance.Rev.E.64/3DGuidanceAPI -fPIC -pthread -m64 -c
CL = g++
CLFLAGS = -m64
CLLIBS = /usr/lib64/ATC3DGlib64.so

CPP = Sample.cpp

OBJ = Sample.o

BIN = Sample64

all: $(BIN)

compile: $(CPP)
	$(CC) $(CCFLAGS) $(CPP)

link: compile $(OBJ)
	$(CL) $(CLFLAGS) $(OBJ) $(CLLIBS) -o $(BIN)

$(BIN): link

